{{#each this}}
  <div class='container-country'>
    <h2>{{name}}</h2>
    <p><b>Capital:</b>{{capital}}</p>
    <p><b>Population:</b>{{population}}</p>
    <p><b>Languages:</b></p>
    <ul>
      {{#each languages}}
        <li>{{name}}</li>
      {{/each}}
    </ul>
  </div>
  <img src='{{flag}}' class='flag' />
{{/each}}